{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-use-before-define": ["error", { "functions": false }],
    "func-names": ["error", "never"],
    "no-shadow": "off",
    "no-console": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "no-plusplus": "off"
  }
}
